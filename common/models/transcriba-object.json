{
  "name": "TranscribaObject",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "title": {
      "type": "string",
      "required": true
    },
    "externalID": {
      "type": "string",
      "required": true
    },
    "status": {
      "type": "number",
      "required": true,
      "default": 0
    },
    "createdAt": {
      "type": "date",
      "required": true
    },
    "released": {
      "type": "boolean",
      "required": true,
      "default": false
    },
    "occupied": {
      "type": "date",
      "required": false
    }
  },
  "validations": [],
  "relations": {
    "source": {
      "type": "belongsTo",
      "model": "Source",
      "foreignKey": ""
    },
    "discussion": {
      "type": "belongsTo",
      "model": "Discussion",
      "foreignKey": ""
    },
    "revisions": {
      "type": "hasMany",
      "model": "Revision",
      "foreignKey": ""
    },
    "active_revision": {
      "type": "hasOne",
      "model": "Revision",
      "foreignKey": "active_revisionId",
      "required": false
    }
  },
  "acls": [],
  "methods": {}
}
