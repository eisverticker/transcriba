{
  "name": "CollectionMapping",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "textRole": {
      "type": "string",
      "default": "none"
    },
    "order": {
      "type": "number",
      "default": 0
    }
  },
  "validations": [],
  "relations": {
    "collection": {
      "type": "belongsTo",
      "model": "Collection",
      "foreignKey": "collectionId"
    },
    "collectionable": {
      "type": "belongsTo",
      "polymorphic": true
    }
  },
  "acls": [],
  "methods": {}
}
